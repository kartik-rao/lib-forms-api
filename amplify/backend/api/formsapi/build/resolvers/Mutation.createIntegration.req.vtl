

## [Start] Set the primary @key. **
#set( $modelObjectKey = {
  "id": $util.dynamodb.toDynamoDB($ctx.args.input.id),
  "integrationTypeId#accountId#formId": $util.dynamodb.toDynamoDB("${ctx.args.input.integrationTypeId}#${ctx.args.input.accountId}#${ctx.args.input.formId}")
} )
## [End] Set the primary @key. **
#if( $util.isNull($dynamodbNameOverrideMap) )
  #set( $dynamodbNameOverrideMap = {
  "integrationTypeId#accountId#formId": "integrationTypeIdAccountIdFormId"
} )
#else
  $util.qr($dynamodbNameOverrideMap.put("integrationTypeId#accountId#formId", "integrationTypeIdAccountIdFormId"))
#end
$util.qr($ctx.args.input.put("integrationTypeId#accountId#formId","${ctx.args.input.integrationTypeId}#${ctx.args.input.accountId}#${ctx.args.input.formId}"))
## [Start] Setting "version" to 1. **
$util.qr($ctx.args.input.put("version", 1))
## [End] Setting "version" to 1. **

## [Start] Prepare DynamoDB PutItem Request. **
$util.qr($context.args.input.put("createdAt", $util.defaultIfNull($ctx.args.input.createdAt, $util.time.nowISO8601())))
$util.qr($context.args.input.put("updatedAt", $util.defaultIfNull($ctx.args.input.updatedAt, $util.time.nowISO8601())))
$util.qr($context.args.input.put("__typename", "Integration"))
{
  "version": "2017-02-28",
  "operation": "PutItem",
  "key": #if( $modelObjectKey ) $util.toJson($modelObjectKey) #else {
  "id":   $util.dynamodb.toDynamoDBJson($util.defaultIfNullOrBlank($ctx.args.input.id, $util.autoId()))
} #end,
  "attributeValues": $util.dynamodb.toMapValuesJson($context.args.input),
  "condition": {
      "expression": "attribute_not_exists(#id)",
      "expressionNames": {
          "#id": "id"
    }
  }
}
## [End] Prepare DynamoDB PutItem Request. **