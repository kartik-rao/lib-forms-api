#set($attribs = $context.arguments.input)
#set($id = $util.autoId())
#set($attribs.meta = "${id}#true")
#set($attribs.owner = $context.identity.sub)
#set($attribs.itemType = "PLAN")
#set($attribs.isDeleted = false)
#set($attribs.createdAt = $util.time.nowISO8601())
#set($attribs.startDate = $attribs.createdAt)
#set($attribs.active = true)
{
    "version": "2017-02-28",
    "operation": "PutItem",
    "key": {
        "id"  : $util.dynamodb.toDynamoDBJson($id),
        "type": {"S": "PLAN"}
    },
    "attributeValues": $util.dynamodb.toDynamoDBJson($attribs),
    "condition": {
        "expression": "attribute_not_exists(#id)",
        "expressionNames": {
            "#id": "id",
        }
    }
}