{
  "name": "@kartikrao/lib-forms-api",
  "version": "1.2.2",
  "description": "GraphQL API for forms app data",
  "main": "lib/index.js",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/kartik-rao/lib-forms-api.git"
  },
  "files": ["lib/**/*"],
  "scripts": {
    "info": "cross-env NODE_ENV=development node_modules/.bin/sls info --stage dev --region ap-northeast-1 --aws-profile fl-infrastructure",
    "compile": "cross-env \"SLS_DEBUG=*\" NODE_ENV=development node_modules/.bin/sls package --stage dev --region ap-northeast-1 --aws-profile fl-infrastructure",
    "deploy": "NODE_ENV=development node_modules/.bin/sls deploy -v --stage dev --region ap-northeast-1 --aws-profile fl-infrastructure",
    "deploy:debug": "cross-env \"SLS_DEBUG=*\" NODE_ENV=development node_modules/.bin/sls deploy -v --stage dev --region ap-northeast-1 --aws-profile fl-infrastructure",
    "remove": "cross-env NODE_ENV=development node_modules/.bin/sls remove -v --stage dev --region ap-northeast-1 --aws-profile fl-infrastructure",
    "deploy:prod": "cross-env NODE_ENV=production node_modules/.bin/sls deploy -v --stage prod --region ap-northeast-1 --aws-profile fl-infrastructure",
    "test": "ts-node -T --project ./tsconfig.test.json node_modules/jasmine/bin/jasmine --config=./jasmine.json",
    "codegen": "graphql-codegen",
    "karma": "karma start",
    "build": "npm run codegen && tsc -p tsconfig.lib.json && cp src/graphql/*.graphql lib/",
    "preversion": "echo \"Skip preversion\"",
    "version": "npm run build && git add -A lib",
    "postversion": "git push && git push --tags",
    "release": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "author": "Kartik Rao",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-amplify/auth": "^1.2.18",
    "@kartikrao/lib-logging-node": "^1.0.2",
    "apollo-cache-inmemory": "^1.1.0",
    "apollo-client": "^2.0.3",
    "apollo-link": "^1.0.3",
    "apollo-link-http": "^1.2.0",
    "aws-amplify": "^1.1.32",
    "aws-appsync": "^1.8.1",
    "aws-sdk": "^2.497.0",
    "aws-xray-sdk": "^2.3.6",
    "data-api-client": "^1.0.0-beta",
    "dayjs": "^1.8.16",
    "es6-promise": "^4.1.1",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.5.0",
    "isomorphic-fetch": "^2.2.1",
    "node-fetch": "^2.6.0",
    "short-uuid": "^3.1.1",
    "source-map-support": "^0.5.12",
    "uuid": "^3.3.2",
    "ws": "^3.3.1"
  },
  "devDependencies": {
    "@anttiviljami/serverless-stack-output": "^0.3.1",
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/typescript": "^1.7.0",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.7.0",
    "@graphql-codegen/typescript-operations": "^1.7.0",
    "@types/aws-lambda": "^8.10.31",
    "@types/graphql": "^14.5.0",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^11.13.2",
    "@types/webpack": "^4.4.25",
    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^5.2.1",
    "aws-cognito-idp-userpool-domain": "^0.1.5",
    "copy-webpack-plugin": "^5.0.0",
    "cross-env": "^5.2.0",
    "generate-password": "^1.4.2",
    "hard-source-webpack-plugin": "^0.13.1",
    "jasmine": "^3.3.1",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "mailslurp-client": "^3.0.25",
    "serverless": "1.51.0",
    "serverless-appsync-offline": "^1.3.3",
    "serverless-appsync-plugin": "^1.1.2",
    "serverless-domain-manager": "^3.3.0",
    "serverless-iam-roles-per-function": "^1.0.4",
    "serverless-log-forwarding": "^1.3.0",
    "serverless-offline": "^4.7.1",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-pseudo-parameters": "^2.4.0",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "typescript": "^3.6.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  }
}
